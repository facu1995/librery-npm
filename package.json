{
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch && yarn storybook",
    "build-lib": "tsdx build",
    "test": "react-scripts test --forceExit",
    "lint": "tsdx lint --fix src && yarn lint-plop",
    "lint-plop": "tsdx lint --fix src plop",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "storybook dev -p 6006",
    "server": "http-server storybook-static",
    "coverage": "npm test -- --coverage --watchAll=false to get coverage",
    "build-storybook": "storybook build",
    "plop": "plop --plopfile plop/plopfile.js"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "always"
  },
  "name": "infosis-components",
  "author": "lautaroRocha",
  "module": "dist/infosis-components.esm.js",
  "size-limit": [
    {
      "path": "dist/infosis-components.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/infosis-components.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@rollup/plugin-image": "3.0.2",
    "@size-limit/preset-small-lib": "8.2.4",
    "@storybook/addon-actions": "7.0.26",
    "@storybook/addon-docs": "7.0.26",
    "@storybook/addon-essentials": "7.0.26",
    "@storybook/addon-interactions": "7.0.26",
    "@storybook/addon-links": "7.0.26",
    "@storybook/addon-mdx-gfm": "7.0.26",
    "@storybook/node-logger": "7.0.26",
    "@storybook/preset-create-react-app": "7.0.26",
    "@storybook/react": "7.0.26",
    "@storybook/react-webpack5": "7.0.26",
    "@storybook/testing-library": "0.2.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "13.5.0",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/yup": "0.32.0",
    "babel-loader": "9.1.2",
    "eslint-plugin-storybook": "0.6.12",
    "plop": "3.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "rollup-plugin-postcss": "4.0.2",
    "size-limit": "8.2.4",
    "storybook": "7.0.26",
    "tsdx": "0.14.1",
    "tslib": "2.5.0",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@storybook/theming": "7.0.26",
    "@types/jest": "27.5.2",
    "@types/node": "20.2.3",
    "date-fns": "2.30.0",
    "formik": "2.2.9",
    "http-server": "14.1.1",
    "react-day-picker": "8.7.1",
    "react-dropzone": "14.2.3",
    "react-infinite-scroll-component": "6.1.0",
    "react-router-dom": "6.14.1",
    "react-scripts": "5.0.1",
    "react-select": "5.7.3",
    "schema-to-yup": "1.12.13"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/components/**/*.tsx",
      "<rootDir>/src/components/**/**/*.tsx"
    ]
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "4.1.1",
    "**/@typescript-eslint/parser": "4.1.1"
  }
}
